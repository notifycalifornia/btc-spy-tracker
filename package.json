{
  "name": "btc-spy-tracker",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start --android --non-interactive || expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:apk:debug": "npx expo prebuild --platform android --non-interactive && cd android && gradlew.bat assembleDebug && cd .. && if not exist artifacts\\android mkdir artifacts\\android && copy /Y android\\app\\build\\outputs\\apk\\debug\\app-debug.apk artifacts\\android\\btc-spy-tracker-debug.apk"
  },
  "dependencies": {
    "expo": "~51.0.0",
    "expo-font": "~12.0.6",
    "expo-status-bar": "~1.12.1",
    "@expo-google-fonts/press-start-2p": "^0.2.3",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-svg": "14.1.0",
    "victory-native": "^36.9.2"
  },
  "overrides": {
    "react-native-svg": "14.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "typescript": "^5.6.2"
  }
}
